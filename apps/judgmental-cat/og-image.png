<svg width="1200" height="630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2a1f1a"/>
      <stop offset="100%" style="stop-color:#1a1210"/>
    </linearGradient>
    <linearGradient id="fur" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e8a44a"/>
      <stop offset="50%" style="stop-color:#d4883a"/>
      <stop offset="100%" style="stop-color:#c67830"/>
    </linearGradient>
    <linearGradient id="eye" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#7cb342"/>
      <stop offset="100%" style="stop-color:#33691e"/>
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bg)"/>

  <!-- Cat head -->
  <ellipse cx="600" cy="380" rx="200" ry="180" fill="url(#fur)"/>

  <!-- Ears -->
  <polygon points="450,250 420,380 500,350" fill="#d4883a"/>
  <polygon points="750,250 780,380 700,350" fill="#d4883a"/>
  <polygon points="455,275 435,350 490,335" fill="#ffb6c1"/>
  <polygon points="745,275 765,350 710,335" fill="#ffb6c1"/>

  <!-- Eyes -->
  <ellipse cx="520" cy="360" rx="45" ry="45" fill="url(#eye)"/>
  <ellipse cx="680" cy="360" rx="45" ry="45" fill="url(#eye)"/>

  <!-- Pupils (narrowed for judgment) -->
  <ellipse cx="520" cy="360" rx="8" ry="35" fill="#000"/>
  <ellipse cx="680" cy="360" rx="8" ry="35" fill="#000"/>

  <!-- Eye glints -->
  <circle cx="535" cy="345" r="10" fill="rgba(255,255,255,0.8)"/>
  <circle cx="695" cy="345" r="10" fill="rgba(255,255,255,0.8)"/>

  <!-- Nose -->
  <ellipse cx="600" cy="430" rx="18" ry="14" fill="#d4756a"/>

  <!-- Mouth -->
  <path d="M570,450 Q600,470 600,450" stroke="#502814" stroke-width="3" fill="none"/>
  <path d="M630,450 Q600,470 600,450" stroke="#502814" stroke-width="3" fill="none"/>

  <!-- Whiskers -->
  <line x1="420" y1="420" x2="520" y2="430" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <line x1="420" y1="440" x2="520" y2="440" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <line x1="420" y1="460" x2="520" y2="450" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <line x1="780" y1="420" x2="680" y2="430" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <line x1="780" y1="440" x2="680" y2="440" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <line x1="780" y1="460" x2="680" y2="450" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>

  <!-- Chest fluff -->
  <ellipse cx="600" cy="530" rx="140" ry="70" fill="#f5c77a"/>

  <!-- Speech bubble -->
  <rect x="750" y="120" width="380" height="120" rx="20" fill="white"/>
  <polygon points="780,240 750,280 820,240" fill="white"/>
  <text x="940" y="175" text-anchor="middle" font-family="Georgia, serif" font-size="24" fill="#333">I'm not mad.</text>
  <text x="940" y="210" text-anchor="middle" font-family="Georgia, serif" font-size="24" fill="#c45a4a" font-style="italic">I'm disappointed.</text>

  <!-- Title -->
  <text x="250" y="180" font-family="Georgia, serif" font-size="56" fill="#f5a623" font-weight="bold">Judgmental</text>
  <text x="250" y="250" font-family="Georgia, serif" font-size="56" fill="#f5a623" font-weight="bold">Cat</text>

  <!-- Subtitle -->
  <text x="250" y="300" font-family="Georgia, serif" font-size="22" fill="#c9a86c" font-style="italic">Your life choices will be evaluated.</text>
</svg>
